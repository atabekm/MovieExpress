<!DOCTYPE html>
<html>
<head>
  <title>MovieExpress</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render "layouts/header" %>

<div class="container">
  <div class="row">

    <% if notice  %>
        <div class="alert alert-dismissible alert-warning"><%= notice %></div>
    <% end %>
    <% if alert %>
        <div class="alert alert-dismissible alert-danger"><%= alert %></div>
    <% end %>

    <%= yield %>

  </div>
</div>


</body>
</html>
